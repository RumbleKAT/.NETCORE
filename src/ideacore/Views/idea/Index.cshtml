@model List<idea>



<h1>아이디어 관리자</h1>

<ul>
    @foreach (var idea in Model)
    {
        <li>
            @idea.id . @idea.Note
        </li>
    }

    <form action="/Idea/Index" method="post">
        <input type="text" name="Note" id="Note" />
        <input type="submit" value="전송"/>
    </form>


</ul>